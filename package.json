{
  "name": "kws-weather-widgets",
  "version": "0.0.3-3",
  "description": "Demonstration Weather Widgets",
  "main": "dist/kwsweather.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "browser": "dist/kwsweather.js",
  "scripts": {
    "build": "stencil build",
    "bump": "node scripts/bump-version.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "release": "node scripts/release.js",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "^0.7.22",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "chalk": "^2.4.1",
    "conventional-changelog": "^1.1.24",
    "execa": "^0.10.0",
    "inquirer": "^5.2.0",
    "jest": "^21.2.1",
    "listr": "~0.14.1",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kensodemann/kws-weather-widgets.git"
  },
  "author": "Ken Sodemann <ken@ionic.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kensodemann/kws-weather-widgets"
  },
  "homepage": "https://github.com/kensodemann/kws-weather-widgets",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
